{
  "name": "vue-svg-fonts",
  "version": "0.1.0",
  "homepage": "https://coding.net/u/zjhiphop/p/cloudtea-iot-fonticon/git",
  "repository": {
    "type": "git",
    "url": "https://coding.net/u/zjhiphop/p/cloudtea-iot-fonticon/git"
  },
  "main": "dist/vue-icon-fonts.js",
  "description": "Build Vue ICON Fonts Components from Sktech file in the fly.",
  "scripts": {
    "start": "gulp watch",
    "symbols": "gulp symbols",
    "installtool": "curl -L https://raw.githubusercontent.com/cognitom/gulp-sketch/master/install-sketchtool.sh | sudo sh",

    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "icons": "node build/fa2json.js && node build/icons.js",
    "fa2json": "node build/fa2json.js",
    "prepublish": "npm run icons && npm run build && cp -r ./src/* . && rm index.js",
    "publish": "rm -rf ./components && rm -rf ./icons"
  },
  "keywords": [
    "Font Awesome",
    "Vue.js",
    "Sketch",
    "icon",
    "font"
  ],
  "private": true,
  "keywords": [
    "symbol font",
    "icon font",
    "sketch",
    "template"
  ],
  "author": "zjhiphop <zjhiphop@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "browser-sync": "^2.18.2",
    "gulp": "^3.9.1",
    "gulp-consolidate": "^0.2.0",
    "gulp-iconfont": "^8.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sketch": "^1.0.3",
    "lodash": "^4.17.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-svg": "^0.4.0",
    "vue-loader": "^11.0.0",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^0.14.1"
  }
}
